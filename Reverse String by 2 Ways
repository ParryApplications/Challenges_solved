package practice;

public class PracticeClass 
{
	public static void main(String args[]) 
	{
		String name = "Paras";
		char char1[] = name.toCharArray();
		for(int i=name.length()-1;i>=0;i--) 
		{
			System.out.print(char1[i]);
		}
	}
	
	/*
	 * OR 
	 */
	
	public static void main(String args[]) 
	{
		String name = "Paras";
		StringBuilder s = new StringBuilder(name);
		s.reverse();
		System.out.print(s);
	}

}
