package practice;

import java.util.Arrays;
import  java.util.Scanner;

public class rough
{
	public static void main(String args[]) 
	{
		int[] array = {8,6,1,0,-1};
		
		Arrays.sort(array);
		for(int i=0;i<array.length;i++) 
		{
			System.out.println("\t"+array[i]);	
		}
		
		System.out.println("ans: " +array[array.length-2]);
	}
}
