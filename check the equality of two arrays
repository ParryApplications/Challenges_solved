package practice;

public class rough
{
	public static void main(String args[]) 
	{
		int[] array1 = {1,2,3,4};
		int[] array2 = {1,2,3,4};
		boolean b = true;
		if(array1.length == array2.length) 
		{
			for(int i=0;i<array1.length;i++) 
			{
				if(array1[i] == array2[i])
					b=true;
				else
					b=false;
			}
		}
		else 
		{
			b = false;
		}
		
		if(b)
		{
			System.out.println("Array is Same");
		}
		else
			System.out.println("Array is Different");
	}
}
